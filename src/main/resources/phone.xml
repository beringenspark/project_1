<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans-3.0.xsd
        http://www.springframework.org/schema/util
        http://www.springframework.org/schema/util/spring-util-2.5.xsd">
    <!-- This file defines phone beans. -->

    <bean name="John_Doe_Cell_Phone" id="cell-1" class="org.practice.XmlBeans.Phone">
        <constructor-arg index="0" value="4441234567"/>
    </bean>
    <bean name="John_Doe_Home_Phone" id="home-1" class="org.practice.XmlBeans.Phone">
        <constructor-arg index="0" value="123456789"/>
    </bean>

</beans>